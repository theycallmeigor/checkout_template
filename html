    <script>
       document.addEventListener('DOMContentLoaded', () => {
      const couponCodeInput1 = document.getElementById('couponCode1');
      // First input
      const couponCodeInput2 = document.getElementById('couponCode2');
      // Second input
      const applyButton1 = document.getElementById('iuco4x');
      // First button
      const applyButton2 = document.getElementById('ijgyon');
      // Second button
      function toggleButton(input, button) {
        if (input && button) {
      	input.addEventListener('input', () => {
      	  const hasValue = input.value.trim() !== '';
      	  button.disabled = !hasValue;
      	  if (hasValue) {
      		button.classList.add('enabled');
      		button.classList.remove('disabled');
      	  }
      	  else {
      		button.classList.add('disabled');
      		button.classList.remove('enabled');
      	  }
      	}
      						  );
        }
      }
      // Apply the function to each input-button pair
      toggleButton(couponCodeInput1, applyButton1);
      toggleButton(couponCodeInput2, applyButton2);
       }
      					   );
    </script>
    <script>
       $("[name=payment_method], [name=fk-payment-option-toggler]").on("change", function(event) {
      openSections(event.target);
       }
      															 );
    </script>
    <script>
      document.addEventListener('DOMContentLoaded', function() {
        const shipAddressInput = document.querySelector('input[name="shipAddress1"]');

        if (shipAddressInput) {
          shipAddressInput.addEventListener('input', function() {
            const fullAddressDiv = document.getElementById('shipping-box');
            const shippingMessage = document.getElementById('enter-shipping-message');

            if (this.value.trim() !== "") {
              fullAddressDiv.style.display = 'block';
              shippingMessage.style.display = 'none';
            } else {
              fullAddressDiv.style.display = 'none';
              shippingMessage.style.display = 'block';
            }
          });
        }
      });
    </script>
  </body>
</html>
